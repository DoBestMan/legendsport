{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "sourceMap": true,
        "lib": ["esnext", "es2017", "dom"],
        "esModuleInterop": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true
    }
}
